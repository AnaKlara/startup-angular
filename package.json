{
  "name": "startup-angular-material",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{ts,js}",
    "lint:fix": "eslint src/**/*.{ts,js} --fix",
    "format": "prettier --write src/**/*.{ts,js,json,css,scss,html}"
  },
  "dependencies": {
    "@angular-eslint/schematics": "^18.0.1",
    "@angular/animations": "^17.3.4",
    "@angular/cdk": "^17.3.4",
    "@angular/common": "^17.3.4",
    "@angular/compiler": "^17.3.4",
    "@angular/core": "^17.3.4",
    "@angular/forms": "^17.3.4",
    "@angular/material": "^17.3.4",
    "@angular/material-moment-adapter": "^17.3.10",
    "@angular/platform-browser": "^17.3.4",
    "@angular/platform-browser-dynamic": "^17.3.4",
    "@angular/router": "^17.3.4",
    "angular-eslint": "^18.0.1",
    "apexcharts": "^3.48.0",
    "material-icons": "^1.13.12",
    "moment": "^2.30.1",
    "ng-apexcharts": "^1.10.0",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.4",
    "@angular/cli": "^17.3.4",
    "@angular/compiler-cli": "^17.3.4",
    "@angular/localize": "^17.3.4",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^48.2.9",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-rxjs": "^5.0.3",
    "eslint-plugin-rxjs-angular": "^2.0.1",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "sass": "^1.75.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1"
    },
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup.jest.ts"
    ],
    "globalSetup": "jest-preset-angular/global-setup",
    "transform": {
      "^.+\\.(js|mjs)$": "jest-preset-angular"
    },
    "testPathIgnorePatterns": [
      "node_modules/(?!@angular/.*|rxjs/.*|zone.js.*)",
      "<rootDir>/dist/"
    ]
  }
}
